{
  "expo": {
    "name": "TUH-Handheld",
    "slug": "TUH-Handheld",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-thu-1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Thu-splash-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mossocelot.handheldapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-thu-1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mossocelot.handheldapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "447c025c-49a7-4be9-9628-008cdeeb58cf"
      }
    }
  }
}
